public class Sum {
    public void run() {
        sum(2, 2);
        sum(10, 400);
    }
    
    private void sum(int a, int b) {
        int total = a + b;
        System.out.println(total);
    }
//Ignore this code-----------------------------------------------------------------------------------------------
  public static void main(String args[]){
  Sum a = new Sum();
  a.run();
  }    
    
}
